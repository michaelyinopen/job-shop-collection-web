{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.4.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.17",
    "clsx": "^1.1.1",
    "date-fns": "^2.22.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-simple-timefield": "^3.2.3",
    "typescript": "^4.3.5",
    "url-template": "^2.0.8",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext ts,tsx,js,jsx src",
    "cosmos": "cross-env FAST_REFRESH=false cosmos",
    "cosmos:export": "cosmos-export",
    "build-stats": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "start-production": "http-server build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "warn",
        "never"
      ],
      "import/no-default-export": [
        "warn"
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "allow": "as-needed",
          "extensions": [
            ".tsx",
            ".jsx"
          ]
        }
      ],
      "@typescript-eslint/consistent-type-imports": [
        "warn",
        {
          "prefer": "type-imports"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/?(*.)+(fixture).[jt]s?(x)"
        ],
        "rules": {
          "import/no-default-export": [
            "off"
          ]
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "resetMocks": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/react-router-dom": "^5.1.7",
    "@types/url-template": "^2.0.28",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.23.4",
    "http-server": "^0.12.3",
    "msw": "^0.33.0",
    "react-cosmos": "^5.6.3",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "proxy": "http://localhost:55758",
  "msw": {
    "workerDirectory": "public"
  }
}
